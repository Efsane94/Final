@model Final_Code_Project.Models.Contact
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="contact_area">
    <div class="container-fluid">
        <div class="row">
            <div class="map_area col-lg-7 col-7 col-12">
                <div class="mapouter">
                    <div class="gmap_canvas">
                        @*@Html.Raw(Model.Map)*@
                        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3039.4283503014667!2d49.85172681476105!3d40.377198065967846!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x40307d077c61fef3%3A0xfa4594c97092ca01!2sAF+Business+House!5e0!3m2!1sen!2s!4v1558348838288!5m2!1sen!2s" width="800" height="600" frameborder="0" style="border:0" allowfullscreen></iframe>
                    </div>
                </div>
            </div>
            <div class="contact_info col-lg-5 col-5 col-12">
                <h2>@Model.Title</h2>
                <p>
                    @Model.Information
                </p>
                <div class="adress_area">
                    <p>
                        <span>address:</span>
                        @Model.Address
                    </p>
                    <p>
                        <span>Telephone</span>
                        @Model.Number
                    </p>
                   
                </div>
            </div>
        </div>
    </div>
</section>

